    "note5" [ label = "对象" ]
    subtitle2 -- note5
    "note6" [ label = "抽象" ]
    note5 -- note6
    "note7" [ label = "属性和行为" ]
    note5 -- note7
    "note8" [ label = "面向过程编程" ]
    subtitle2 -- note8
